SOURCE_DIR = 

TARGET_ARCH=x64Darwin16clang8.0

# If undefined in the environment default NDDSHOME to install dir
ifndef NDDSHOME
NDDSHOME := /Applications/rti_connext_dds-5.3.0
endif

JAVA_PATH = java
JAVAC_PATH = javac

JAVA_SOURCES = $(SOURCE_DIR)./Sync.java $(SOURCE_DIR)./SyncSeq.java $(SOURCE_DIR)./SyncTypeSupport.java $(SOURCE_DIR)./SyncTypeCode.java $(SOURCE_DIR)./SyncDataReader.java $(SOURCE_DIR)./SyncDataWriter.java $(SOURCE_DIR)./SyncSubscriber.java $(SOURCE_DIR)./SyncPublisher.java
CLASS_FILES  = $(JAVA_SOURCES:%.java=%.class)

RTI_CLASSPATH := $(NDDSHOME)/lib/java/nddsjava.jar

%.class : %.java
	$(JAVAC_PATH)  -classpath ".:$(RTI_CLASSPATH)" $<

all: $(CLASS_FILES)
 
#
# Convenient way to run the java programs
#

export DYLD_LIBRARY_PATH := $(NDDSHOME)/lib/x64Darwin16clang8.0:/usr/lib/lwp:$(DYLD_LIBRARY_PATH)

SyncPublisher: ./SyncPublisher.class
	$(JAVA_PATH) -d64 -Djava.library.path=$(NDDSHOME)/lib/x64Darwin16clang8.0 -classpath ".:$(RTI_CLASSPATH)" SyncPublisher $(ARGS)
 
SyncSubscriber: ./SyncSubscriber.class
	$(JAVA_PATH) -d64 -Djava.library.path=$(NDDSHOME)/lib/x64Darwin16clang8.0 -classpath ".:$(RTI_CLASSPATH)" SyncSubscriber $(ARGS)
